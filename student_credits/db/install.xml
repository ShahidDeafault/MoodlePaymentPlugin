<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="MoodlePaymentPlugin/student_credits/db" VERSION="20150402" COMMENT="XMLDB file for Moodle /student_credits"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
	
  <TABLES>
    <TABLE NAME="credits" COMMENT="">
        <FIELDS>
            <FIELD NAME="id"            TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
            <FIELD NAME="username"  	TYPE="char"	 LENGTH="100" NOTNULL="true" SEQUENCE="false" ENUM="false" />
            <FIELD NAME="creditamount"  TYPE="char" LENGTH="15" NOTNULL="true" SEQUENCE="false" ENUM="false" />
            <FIELD NAME="comments"      TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" />
        </FIELDS>
        <KEYS>
              <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
	